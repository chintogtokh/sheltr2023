{
  "name": "sheltr-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "d3": "^5.0.2",
    "d3v3": "^1.0.3",
    "leaflet": "^1.3.1",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-bootstrap": "^0.32.1",
    "react-d3-radar": "^0.2.6",
    "react-dom": "^16.3.1",
    "react-js-pagination": "^3.0.2",
    "react-leaflet": "^1.9.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.0",
    "react-scripts": "1.1.1",
    "react-select": "^1.2.1",
    "react-streetview": "^1.1.1",
    "react-toastify": "^3.4.3",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "turf-bbox": "^3.0.12"
  },
  "scripts": {
    "build-css": "npx node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --use-polling",
    "start-js": "CHOKIDAR_USEPOLLING=true PORT=3000 react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm run build-css && npm run build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
